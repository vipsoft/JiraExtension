{
    "name": "vipsoft/jira-extension",
    "type": "library",
    "description": "Load features for Behat from Jira issues",
    "keywords": ["bdd", "behat", "jira", "feature"],
    "homepage": "http://github.com/vipsoft/",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthon Pang",
            "email": "apang@softwaredevelopment.ca",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.3.6",
        "behat/behat": "~2.4",
        "symfony/config": "~2.0",
        "symfony/dependency-injection": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": { "VIPSoft\\JiraExtension": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev",
            "dev-2.5": "0.10.x-dev"
        }
    }
}
